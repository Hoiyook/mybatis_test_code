<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="context1">
  <!-- 注释的配信息 -->
    <commentGenerator>
      <property name="suppressAllComments" value="true"/>
    </commentGenerator>
    <!-- 配置数据库的连接 -->
    <jdbcConnection  connectionURL="jdbc:mysql://localhost:3306/school" driverClass="org.gjt.mm.mysql.Driver" password="123456" userId="root" />
    <!-- POJO -->
    <javaModelGenerator targetPackage="cn.itheima.pojo" targetProject="mybatis-demo13" />
    <!-- 映射文件 -->
    <sqlMapGenerator targetPackage="cn.itheima.mapper" targetProject="mybatis-demo13" />
    <!-- 操作接口 -->
    <javaClientGenerator targetPackage="cn.itheima.mapper" targetProject="mybatis-demo13" type="XMLMAPPER" />
    <table enableCountByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"  tableName="student"></table>
    <table enableCountByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false"  tableName="score"></table>
  </context>
</generatorConfiguration>